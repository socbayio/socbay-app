{
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
        "no-unused-vars": "warn",
        "no-plusplus": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "warn",
        "eqeqeq": "warn",
        "no-await-in-loop": "warn",
        "no-undef": "warn", //some global vars are recongnized as undef
        "prefer-destructuring": "warn"
    }
}
